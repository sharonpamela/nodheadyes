  "dependencies": {
    "concurrently": "^4.1.0",
    "cookie-session": "^1.3.3",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.6.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0"
